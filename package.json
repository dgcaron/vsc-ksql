{
    "name": "ksql",
    "displayName": "ksql",
    "description": "Language support for KSQL",
    "version": "0.0.1",
    "publisher": "rmoff",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ksql",
